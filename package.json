{
  "name": "apple-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production npm run build:js && npm run build:css && npm run build:html",
    "build:css": "ncp src/style.css dist/style.css",
    "build:html": "ncp src/index.html dist/index.html",
    "build:js": "browserify src/index.js -t babelify | uglifyjs --compress --mangle > dist/build.js",
    "dev": "NODE_ENV=development npm run watch & npm run serve:dev",
    "lint": "eslint src test",
    "prebuild": "rimraf dist && mkdirp dist",
    "predev": "rimraf dev && mkdirp dev",
    "serve:dist": "http-server dist",
    "serve:dev": "http-server dev",
    "start": "npm run build && npm run serve:dist",
    "test": "babel-node test/index.js",
    "watch": "npm run watch:js & npm run watch:css & npm run watch:html",
    "watch:html": "chokidar --initial 'src/*.html' -c 'ncp src/index.html dev/index.html'",
    "watch:js": "watchify src/index.js -t babelify --debug --verbose -o dev/build.js",
    "watch:css": "chokidar --initial 'src/*.css' -c 'ncp src/style.css dev/style.css'",
    "watch:test": "chokidar --initial 'src/**/*.js' 'spec/*.js' -c 'npm test'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^0.14.0-rc1",
    "react-document-title": "^2.0.0",
    "react-dom": "^0.14.0-rc1",
    "react-motion": "^0.2.7",
    "react-progress-button": "^1.1.0",
    "react-router": "^1.0.0-rc1",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.2",
    "babelify": "^6.3.0",
    "browserify": "^11.1.0",
    "browserify-hmr": "^0.2.2",
    "chokidar-cli": "^1.0.1",
    "eslint": "^1.4.1",
    "eslint-config-defaults": "^6.0.0",
    "eslint-plugin-react": "^3.3.2",
    "http-server": "^0.8.0",
    "inliner": "^1.3.0",
    "jsdom": "^6.4.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "react-addons-test-utils": "^0.14.0-rc1",
    "react-hot-transform": "^1.0.2",
    "rimraf": "^2.4.3",
    "tape": "^4.2.0",
    "watchify": "^3.4.0"
  }
}
